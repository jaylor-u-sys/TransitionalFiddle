{
  "name": "TransitionalFiddle",
  "version": "1.0.0",
  "scripts": {
    "start": "tsc && node-sass app/ -o app/ && node-sass app/* -o app/* && concurrently \"npm run sass:w\" \"npm run tsc:w\" \"npm run lite\" ",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "sass": "node-sass app/ -o app/ && node-sass app/* -o app/*",
    "sass:w": "node-sass -w app/ -o app/",
    "lite": "lite-server",
    "typings": "typings",
    "postinstall": "typings install"
  },
  "license": "ISC",
  "dependencies": {
    "angular2": "^2.0.0-beta.14",
    "es6-shim": "^0.35.0",
    "reflect-metadata": "^0.1.2",
    "rxjs": "^5.0.0-beta.2",
    "systemjs": "^0.19.25",
    "zone.js": "^0.6.6"
  },
  "devDependencies": {
    "concurrently": "^2.0.0",
    "lite-server": "^2.2.0",
    "typescript": "^1.8.9",
    "typings": "^0.7.12",
    "node-sass": "^3.4.2"
  }
}
